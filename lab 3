syms s w b
a=(w^2)/(s^2+2*b*w*s+w^2)*(1/s)
ilaplace(a)

syms a b c x
y=(a*x^2+b*x+c)==0
p=solve(y)

clc;clear all;
syms  y(t)
dsolve(diff(y)==sin(t))
dsolve(diff(y)==sin(t),y(0)==2)

clc;clear all;
syms  x
int(sin(x),pi/2,3*pi/2)

clc;clear all;
syms x
simplify(sin(x)^2 + cos(x)^2)

clc;
clear all;
syms t p
p=2
assume(t>0)
c=1-exp(-t/p)
fplot(c)

clc;
clear all;
syms x
a=[x+1 x^2;0 3]
b=inv(a)
subs(b,x,1)


